<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo-sim.org/xsd/netgenerateConfiguration.xsd">

    <grid_network>
        <grid value="true"/>
        <grid.number value="4"/>
        <grid.length value="200"/>
    </grid_network>

    <output>
        <output-file value="fanjing.net.xml"/>
        <plain-output-prefix value="fanjing"/>
    </output>

    <processing>
        <no-turnarounds value="true"/>
        <no-turnarounds.tls value="true"/>
    </processing>

    <building_defaults>
        <default-junction-type value="traffic_light"/>
    </building_defaults>

</configuration>
